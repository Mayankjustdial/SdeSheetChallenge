#include<bits/stdc++.h>
double median(vector<int> a, vector<int> b)
{
    // Write your code here.
     for(int i = 0; i < b.size(); i++)
            a.push_back(b[i]);
        
        sort(a.begin(), a.end());
        int size = a.size();
        if(size % 2 != 0)
            return a[size/2];
        else
            return (a[size/2] + a[size/2-1])/2.00000;
}

